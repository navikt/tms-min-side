---
import Layout from "../../layouts/base/Layout.astro";
import TidligereVarsler from "../../mikrofrontends/tidligere-varsler/TidligereVarsler";
import Content from "../../layouts/content/Content.astro";
import { getLanguage } from "@language/language";
import { text } from "@language/text";

const language = getLanguage(Astro.url);
const breadcrumbs = [
  {
  title: text.minSide[language],
  url: "/minside"
  },
  {
    url: `/minside/tidligere-varsler`,
    title: text.tidligereVarslinger[language],
    handleInApp: true,
  }
];
---

<Layout title="Min side" breadcrumbs={breadcrumbs}>
  <Content cls="content">
    <TidligereVarsler language={language} client:only="react" />
  </Content>
</Layout>
