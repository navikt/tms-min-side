---
import Layout from "../../layouts/base/Layout.astro";
import Arbeidssoker from "../../mikrofrontends/arbeidssoker/Arbeidssoker";
import Content from "../../layouts/content/Content.astro";
import { getLanguage } from "@language/language";
import { text } from "@language/text";

const language = getLanguage(Astro.url);
const breadcrumbs = [
  {
    title: text.minSide[language],
    url: "/minside"
  },
  {
    url: `/minside/arbeidssoker`,
    title: text.arbeidssoker[language],
    handleInApp: true,
  },
];
---

<Layout title="Min side" breadcrumbs={breadcrumbs}>
  <Content cls="content">
    <Arbeidssoker language={language} client:only="react" />
  </Content>
</Layout>
